                <LinearLayout
                    android:layout_width="@dimen/transaction_printreceipt_row_width4_double"
                    android:layout_height="match_parent"
                    />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/transaction_printreceipt_padding_small_double" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/transaction_printreceipt_padding_smallest_double"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:layout_width="@dimen/transaction_printreceipt_row_width4_double"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    >
                    <com.spectratech.lib.view.HKFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="left"
                        android:textColor="@color/transaction_printreceipt_printtext"
                        android:textSize="@dimen/transaction_printreceipt_textsize_littlemore_double"
                        android:text="@string/transaction_printreceipt_total"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="@dimen/transaction_printreceipt_padding_smallest_double"
                    android:orientation="horizontal"
                    >
                    <com.spectratech.lib.view.HKFontTextView
                        android:id="@+id/val_amount_total"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="right"
                        android:textColor="@color/transaction_printreceipt_printtext"
                        android:textSize="@dimen/transaction_printreceipt_textsize_littlemore_double"
                        android:maxLines="1"
                        android:lines="1"
                        android:text="@string/transaction_printreceipt_val_default"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/offline_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/transaction_printreceipt_padding_smallest_double"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:visibility="gone"
                >
                <com.spectratech.lib.view.HKFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_littlemore_double"
                    android:text="@string/transaction_printreceipt_offline"
                    />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/transaction_printreceipt_padding_large_double" />

            <!--canceled-->
            <include layout="@layout/transaction_printreceipt_canceled_double" />

            <!--declined-->
            <include layout="@layout/transaction_printreceipt_declined_double" />

            <!--no signature-->
            <include layout="@layout/transaction_printreceipt_nosign_double" />

            <LinearLayout
                android:id="@+id/container_signature"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <com.spectratech.lib.view.HKFontTextView
                        android:layout_width="@dimen/transaction_printreceipt_signature_cross_width_double"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="bottom|right"
                        android:textColor="@color/transaction_printreceipt_printtext"
                        android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                        android:text="X"
                        />
                    <com.spectratech.lib.view.DrawableLinearLayout
                        android:id="@+id/transaction_printreceipt_signature"
                        android:layout_width="@dimen/transaction_printreceipt_header5_content_width_double"
                        android:layout_height="@dimen/transaction_printreceipt_header5_content_height_double"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-10px"
                    android:layout_marginLeft="@dimen/transaction_printreceipt_margin_large_double"
                    android:layout_marginRight="@dimen/transaction_printreceipt_margin_large_double"
                    >
                    <include layout="@layout/transaction_printreceipt_signunderline_double" />
                </LinearLayout>
                <com.spectratech.lib.view.HKFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:text="@string/transaction_printreceipt_cardholdersign"
                    />
                <include layout="@layout/transaction_printreceipt_divider_wmargin_double" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/transaction_printreceipt_padding_small_double" />

            <!--approval code-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <com.spectratech.lib.view.HKFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:text="@string/transaction_printreceipt_approvalcode_with_colon"
                    />
                <View
                    android:layout_width="@dimen/transaction_printreceipt_padding_smaller_double"
                    android:layout_height="match_parent" />
                <com.spectratech.lib.view.HKFontTextView
                    android:id="@+id/val_appcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:maxLines="1"
                    android:lines="1"
                    android:text="@string/transaction_printreceipt_val_default"
                    />
            </LinearLayout>

            <!--APP-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <com.spectratech.lib.view.HKFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:text="@string/transaction_printreceipt_app_with_colon"
                    />
                <View
                    android:layout_width="@dimen/transaction_printreceipt_padding_smaller_double"
                    android:layout_height="match_parent" />
                <com.spectratech.lib.view.HKFontTextView
                    android:id="@+id/val_app"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:maxLines="1"
                    android:lines="1"
                    android:text=""
                    />
            </LinearLayout>

            <!--AID-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <com.spectratech.lib.view.HKFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:text="@string/transaction_printreceipt_aid_with_colon"
                    />
                <View
                    android:layout_width="@dimen/transaction_printreceipt_padding_smaller_double"
                    android:layout_height="match_parent" />
                <com.spectratech.lib.view.HKFontTextView
                    android:id="@+id/val_aid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:maxLines="1"
                    android:lines="1"
                    android:text="@string/transaction_printreceipt_val_default"
                    />
            </LinearLayout>

            <!--TC-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <com.spectratech.lib.view.HKFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:text="@string/transaction_printreceipt_tc_with_colon"
                    />
                <View
                    android:layout_width="@dimen/transaction_printreceipt_padding_smaller_double"
                    android:layout_height="match_parent" />
                <com.spectratech.lib.view.HKFontTextView
                    android:id="@+id/val_tc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_generaladdtwo_double"
                    android:maxLines="1"
                    android:lines="1"
                    android:text="@string/transaction_printreceipt_val_default"
                    />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/transaction_printreceipt_padding_large_double" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <com.spectratech.lib.view.HKFontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_tailagreement_textsize_double"
                    android:text="@string/transaction_printreceipt_tail_text"
                    />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/transaction_printreceipt_row_height1_double" />

        </LinearLayout>
    </LinearLayout>
</com.spectratech.sp530demo.TransactionPrintReceiptLinearLayout>