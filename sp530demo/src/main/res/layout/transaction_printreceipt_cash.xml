            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/transaction_printreceipt_padding_general_double" />

            <!--MID-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:layout_width="@dimen/transaction_printreceipt_row_width4_double"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    >
                    <com.spectratech.lib.view.HKFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="left"
                        android:textColor="@color/transaction_printreceipt_printtext"
                        android:textSize="@dimen/transaction_printreceipt_textsize_littlemore_double"
                        android:text="@string/transaction_printreceipt_mid"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="@dimen/transaction_printreceipt_padding_smallest_double"
                    android:orientation="horizontal"
                    >
                    <com.spectratech.lib.view.HKFontTextView
                        android:id="@+id/val_mid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="right"
                        android:textColor="@color/transaction_printreceipt_printtext"
                        android:textSize="@dimen/transaction_printreceipt_textsize_littlemore_double"
                        android:maxLines="1"
                        android:lines="1"
                        android:text="@string/transaction_printreceipt_val_default"
                        />
                </LinearLayout>
            </LinearLayout>

            <!--TID-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:layout_width="@dimen/transaction_printreceipt_row_width4_double"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    >
                    <com.spectratech.lib.view.HKFontTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="left"
                        android:textColor="@color/transaction_printreceipt_printtext"
                        android:textSize="@dimen/transaction_printreceipt_textsize_littlemore_double"
                        android:text="@string/transaction_printreceipt_tid"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="@dimen/transaction_printreceipt_padding_smallest_double"
                    android:orientation="horizontal"
                    >
                    <com.spectratech.lib.view.HKFontTextView
                        android:id="@+id/val_tid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="right"
                        android:textColor="@color/transaction_printreceipt_printtext"
                        android:textSize="@dimen/transaction_printreceipt_textsize_littlemore_double"
                        android:maxLines="1"
                        android:lines="1"
                        android:text="@string/transaction_printreceipt_val_default"
                        />
                </LinearLayout>
            </LinearLayout>

            <!--separator-->
            <include layout="@layout/transaction_printreceipt_divider_wmargin_double" />



            <!--card type-->

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/transaction_printreceipt_padding_small_double" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="@dimen/transaction_printreceipt_margin_small"
                >
                <com.spectratech.lib.view.HKFontTextView
                    android:id="@+id/val_txBig"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:gravity="left"
                    android:textColor="@color/transaction_printreceipt_printtext"
                    android:textSize="@dimen/transaction_printreceipt_textsize_large_double"
                    android:maxLines="1"
                    android:lines="1"
                    android:text="SALE"
                    />
            </LinearLayout>

            <LinearLayout
                android:baselineAligned="false"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="0.5"
                    android:background="#FFFFFF"
                    android:paddingBottom="1dip"
                    android:paddingLeft="1dip"
                    android:paddingTop="1dip" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="2"
                        android:background="#000011"
                        android:gravity="center"
                        android:paddingRight="3dp"
                        android:text="@string/name"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="0.2"
                    android:background="#FFFFFF"
                    android:paddingBottom="1dip"
                    android:paddingLeft="1dip"
                    android:paddingTop="1dip" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.5"
                        android:background="#000011"
                        android:gravity="center"
                        android:paddingRight="3dp"
                        android:text="@string/quantity"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold" />
                </TableRow>

                <TableRow
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.2"
                    android:background="#FFFFFF"
                    android:paddingBottom="1dip"
                    android:paddingLeft="1dip"
                    android:paddingRight="1dip"
                    android:paddingTop="1dip" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="0.75"
                        android:background="#000011"
                        android:gravity="center"
                        android:paddingRight="3dp"
                        android:textSize="12sp"
                        android:text="@string/unit_price"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold" />
                </TableRow>
            </LinearLayout>

            <!--amount-->

            <ListView
                android:id="@+id/sale_ListPrint1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >
            </ListView>

            <!--tip-->


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/transaction_printreceipt_padding_smallest_double"
                android:orientation="horizontal"
